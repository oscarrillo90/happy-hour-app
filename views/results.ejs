
<% for(var i=0; i< results.length; i++) {  %>
       <form class="form-wrapper3 cf">
            <img class="yelpimg"    id="beer" src= <%= results[i].image_url %> alt="Card image cap">
            <div class="celltitle">
              <span id="text"><h2 class="card-title"><a href= "show/<%= results[i]._id %>"><%= results[i].name %></a></h2></span>
            </div>
            <div class="cell">
              <span id="text"><h4 class="rating"><%= results[i].rating %></h4></span>
            </div>
            <div class="cell">
              <span id="text"><h4 class="type"><%= results[i].categories %></h4></span>
            </div>
            <div class="cell">
              <span id="text"><h4 class="address"><%= results[i].location %></h4></span>
            </div>
        </form>
<% } %>
