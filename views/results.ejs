<% for(var i=0; i< results.length; i++) {  %>
            <div class="event card">
                  <div class="card-img-left">
                      <img class="card-img-left" src= <%= results[i].image_url %> alt="Card image cap">
                  </div>
                  <div class="card-block">
                      <h2 class="card-title"><a href= "show/<%= results[i]._id %>"><%= results[i].name %></a></h2>
                      <h4 class="rating"><%= results[i].rating %></h4>
                      <h4 class="price"><%= results[i].price %></h4>
                      <h4 class="type"><%= results[i].categories %></h4>
                      <h4 class="address"><%= results[i].location %></h4>
                  </div>
            </div>
			<% } %>
